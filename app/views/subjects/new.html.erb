<% @page_title = "New Subject" %>
<% @panel_header = "Create New Subject" %> 
<div class="row"> 
    <div class="col-lg-12">
      <%= link_to("<<< Go To List", {:action => 'index'}) %> 
    </div>
</div>
<div class="row">
   <%= error_message_for(@subject) %>
   <div class="col-lg-4"></div>
   <div class="col-lg-4">
      <%= form_for(:subject, :url => {:action => 'create'}) do |f| %>
         <%= render(:partial => "form", :locals => {:f => f}) %>
         <div class="col-sm-offset-0"> 
            <%= submit_tag('Create New Subject',:class => 'btn btn-default')%> 
         </div>
      <% end %> 
   </div>
   <div class="col-lg-4"> 
   </div>
</div>
   
